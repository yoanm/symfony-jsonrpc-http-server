{
    "name": "yoanm/symfony-jsonrpc-http-server",
    "description": "Symfony Server SDK to convert an HTTP json-rpc request into HTTP json-rpc response",
    "license": "GPL-3.0-only",
    "type": "library",
    "support": {
        "issues": "https://github.com/yoanm/symfony-jsonrpc-http-server/issues"
    },
    "authors": [
        {
            "name": "Yoanm",
            "email": "yoanm@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yoanm\\SymfonyJsonRpcHttpServer\\App\\": "src/App",
            "Yoanm\\SymfonyJsonRpcHttpServer\\Domain\\": "src/Domain",
            "Yoanm\\SymfonyJsonRpcHttpServer\\Infra\\": "src/Infra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "Tests\\Functional\\BehatContext\\": "features/bootstrap"
        }
    },
    "require": {
        "php": ">7.0",
        "yoanm/jsonrpc-server-sdk": "^1.0",
        "yoanm/jsonrpc-server-sdk-psr11-resolver": "^1.0",
        "symfony/http-foundation": "^3.0 || ^4.0"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^6.0"
    }
}
